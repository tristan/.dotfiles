xterm*Background:       black
xterm*Foreground:        green
xterm*cursorColor:      LightBlue
xterm*visualBell: false

 ! General
! urxvt*termName:  rxvt-256color
 urxvt*loginShell:         true
 urxvt*scrollBar:         false
 urxvt*secondaryScroll:    true
 urxvt*saveLines:         65535
 urxvt*cursorBlink:       false
 urxvt*urgentOnBell:       true
 urxvt*fontMenu.Label:  Unicode Fonts
 urxvt*utf8:    1
 !urxvt*font2:  -misc-fixed-medium-r-normal--8-80-75-75-c-50-iso10646-1
 !urxvt*font:   -misc-fixed-medium-r-semicondensed--13-120-75-75-c-60-iso10646-1
 !urxvt*font3:  -misc-fixed-medium-r-normal--14-130-75-75-c-70-iso10646-1
 !urxvt*font4:  -misc-fixed-medium-r-normal--13-120-75-75-c-80-iso10646-1
 !urxvt*font5:  -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1
 !urxvt*font6:  -misc-fixed-medium-r-normal--20-200-75-75-c-100-iso10646-1
 !urxvt*override-redirect:false
 !urxvt*borderLess:       false
 !urxvt*internalBorder:       0
 !urxvt*externalBorder:       0

urxvt.font: xft:Inconsolata:size=10

 ! Extensions
 urxvt*perl-lib:        /usr/lib/urxvt/perl/
 urxvt*perl-ext-common: default,matcher,searchable-scrollback,resize-font
 urxvt*urlLauncher:     /usr/bin/firefox
 urxvt*matcher.button:  1

 !  - catch ugly URLs
 urxvt.cutchars:        `()'*<>[]{|}"
 urxvt.perl-ext-common: ...,tabbed,resize-font,...

